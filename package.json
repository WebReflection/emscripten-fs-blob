{
  "name": "emscripten-fs-blob",
  "version": "0.1.0",
  "description": "Emscripten FS as sharable and recoverable Blob",
  "main": "src/index.js",
  "module": "src/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.js",
      "types": "./types/index.d.ts"
    },
    "./from": {
      "import": "./src/from.js",
      "types": "./types/from.d.ts"
    },
    "./to": {
      "import": "./src/to.js",
      "types": "./types/to.d.ts"
    },
    "package.json": "./package.json"
  },
  "files": [
    "src/",
    "types/",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "emscripten",
    "fs",
    "blob",
    "compress",
    "binary"
  ],
  "author": "Andrea Giammarchi",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "types": "rm -rf types && tsc --allowJs --checkJs --lib dom,esnext --moduleResolution nodenext --module NodeNext --target esnext -d --emitDeclarationOnly --outDir ./types ./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/emscripten-fs-blob.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/emscripten-fs-blob/issues"
  },
  "homepage": "https://github.com/WebReflection/emscripten-fs-blob#readme",
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
